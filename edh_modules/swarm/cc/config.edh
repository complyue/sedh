{##
 # this module is defined so the configuration is customizable with a nested
 # .edh file in edh_modules besides cwd of a tool
 #}
export {

  {## configure registry directory #}
  regDir = './etc'

  {## management page & pixie api #}
  httpPort = 6780 httpInterface = '0.0.0.0'

  {## heartbeat sniffing #}
  hbPort = 6768 hbInterface = '0.0.0.0'

}
